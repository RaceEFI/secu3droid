<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadeScrollbars="false">
    
	<RelativeLayout
	    android:id="@+id/layout1"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical" >

	    <TextView
	        android:id="@+id/textView1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentTop="true"
	        android:text="Overrun low threshhold (gasoline)"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/carburOverrunLowThreshholdGasolineEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/textView1"
	        android:ems="10"
	        android:inputType="number"
	        android:text="0" />

	    <TextView
	        android:id="@+id/textView2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/carburOverrunLowThreshholdGasolineEditText"
	        android:text="Overrun high threshhold (gasoline)"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/carburOverrunHighThreshholdGasolineEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/textView2"
	        android:ems="10"
	        android:inputType="number"
	        android:text="0" >

	        <requestFocus />
	    </EditText>

	    <TextView
	        android:id="@+id/textView3"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/carburOverrunHighThreshholdGasolineEditText"
	        android:text="Overrun low threshhold (gas)"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/carburOverrunLowThreshholdGasEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/textView3"
	        android:ems="10"
	        android:inputType="number"
	        android:text="0" />

	    <TextView
	        android:id="@+id/textView4"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/carburOverrunLowThreshholdGasEditText"
	        android:text="Overrun high threshhold (gas)"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/carburOverrunHighThreshholdGasEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/textView4"
	        android:ems="10"
	        android:inputType="number"
	        android:text="0" />

	    <TextView
	        android:id="@+id/textView5"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/carburOverrunHighThreshholdGasEditText"
	        android:text="Overrun valve delay"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/carburOverrunValveDelayEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/textView5"
	        android:ems="10"
	        android:inputType="numberDecimal"
	        android:text="0.0" />

	    <CheckBox
	        android:id="@+id/carburSensorInverseCheckBox"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/carburOverrunValveDelayEditText"
	        android:text="Carburator sensor inverse"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <TextView
	        android:id="@+id/textView6"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/carburSensorInverseCheckBox"
	        android:text="EPM valve ON pressure"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/carburEPMValveOnPressureEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/textView6"
	        android:ems="10"
	        android:inputType="numberDecimal"
	        android:text="0.0" />

	    <TextView
	        android:id="@+id/textView7"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBaseline="@+id/carburOverrunLowThreshholdGasolineEditText"
	        android:layout_alignBottom="@+id/carburOverrunLowThreshholdGasolineEditText"
	        android:layout_toRightOf="@+id/carburOverrunLowThreshholdGasolineEditText"
	        android:text="@string/units_rpm"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <TextView
	        android:id="@+id/textView8"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBaseline="@+id/carburOverrunHighThreshholdGasolineEditText"
	        android:layout_alignBottom="@+id/carburOverrunHighThreshholdGasolineEditText"
	        android:layout_toRightOf="@+id/carburOverrunHighThreshholdGasolineEditText"
	        android:text="@string/units_rpm"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <TextView
	        android:id="@+id/textView9"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBaseline="@+id/carburOverrunLowThreshholdGasEditText"
	        android:layout_alignBottom="@+id/carburOverrunLowThreshholdGasEditText"
	        android:layout_toRightOf="@+id/carburOverrunLowThreshholdGasEditText"
	        android:text="@string/units_rpm"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <TextView
	        android:id="@+id/textView10"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBaseline="@+id/carburOverrunHighThreshholdGasEditText"
	        android:layout_alignBottom="@+id/carburOverrunHighThreshholdGasEditText"
	        android:layout_toRightOf="@+id/carburOverrunHighThreshholdGasEditText"
	        android:text="@string/units_rpm"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <TextView
	        android:id="@+id/textView11"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBaseline="@+id/carburOverrunValveDelayEditText"
	        android:layout_alignBottom="@+id/carburOverrunValveDelayEditText"
	        android:layout_toRightOf="@+id/carburOverrunValveDelayEditText"
	        android:text="@string/units_sec"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <TextView
	        android:id="@+id/textView12"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBaseline="@+id/carburEPMValveOnPressureEditText"
	        android:layout_alignBottom="@+id/carburEPMValveOnPressureEditText"
	        android:layout_toRightOf="@+id/carburEPMValveOnPressureEditText"
	        android:text="@string/units_pressure_kpa"
	        android:textAppearance="?android:attr/textAppearanceLarge" />
	
	</RelativeLayout>
	
</ScrollView>	