<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadeScrollbars="false">
    
	<RelativeLayout
	    android:id="@+id/paramsLayout"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical" >

	    <TextView
	        android:id="@+id/adccorTextView1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentTop="true"
	        android:text="@string/adccor_map_sensor_factor_title"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/adccorMAPSensorCoefficientEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/adccorTextView1"
	        android:ems="10"
	        android:inputType="numberSigned"
	        android:text="@string/units_zero_float" >

	        <requestFocus />
	    </EditText>

	    <TextView
	        android:id="@+id/adccorTextView2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/adccorMAPSensorCoefficientEditText"
	        android:layout_marginTop="16dp"
	        android:text="@string/adccor_map_sensor_correction_title"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/adccorMAPSensorCorrectionEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@+id/adccorTextView2"
	        android:ems="10"
	        android:inputType="numberSigned"
	        android:text="@string/units_zero_float" />

	    <TextView
	        android:id="@+id/adccorTextView3"
