	        android:text="@string/adccor_addi2_sensor_correction_title"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

	    <EditText
	        android:id="@+id/adccorADDIO2SensorCorrectionEditText"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_below="@id/adccorTextView12"
	        android:ems="10"
	        android:inputType="numberDecimal"
	        android:text="@string/units_zero_float" />
	
