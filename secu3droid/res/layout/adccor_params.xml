	        android:inputType="numberDecimal"
	        android:text="@string/units_zero_float" />

	    <TextView
	        android:id="@+id/adccorTextView8"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
